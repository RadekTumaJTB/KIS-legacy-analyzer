<ViewObject
   Name="KpDatDokladzdrojdatagregaceVazby1View"
   OrderBy="id_doklad,ID_KTGUCETNISPOLECNOSTTO,s_ucetUnif,S_MENA"
   BindingStyle="Oracle"
   CustomQuery="true"
   ComponentClass="cz.jtbank.konsolidace.doklady.KpDatDokladzdrojdatagregaceVazby1ViewImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
select v.id_doklad,
	   uu.S_UCET s_ucetUnif,
	   v.S_MENA,
	   sum(decode(uu.c_class,'A',1,'3',1,-1)*v.ND_CASTKALOCAL) ND_CASTKALOCAL,
	   sum(decode(uu.c_class,'A',1,'3',1,-1)*v.ND_CASTKAMENA) ND_CASTKAMENA,
	   v.ID_KTGUCETNISPOLECNOSTTO,
	   v.ID_KTGUCETNISKUPINA,
      f.s_kod
from db_jt.kp_dat_dokladVazby v,
	 db_jt.kp_dat_doklad d,
	 db_jt.kp_cis_ucetUnif uu,
	db_jt.kp_rel_ucetunifflow rf,
	db_jt.kp_cis_flow f
where v.id_doklad = d.id 
  and v.ID_KTGUCETNISPOLECNOSTFROM = d.ID_KTGUCETNISPOLECNOST
  and NVL ( v.NL_USERINDEXVAZBA, 0 ) = 0
  and v.ID_CISUCETUNIF = uu.ID and
	rf.id_cisflow = f.id   and
	uu.id = rf.id_cisucetunif  
group by v.id_doklad,
		 v.S_MENA,
		 v.ID_KTGUCETNISPOLECNOSTTO,
		 v.ID_KTGUCETNISKUPINA,
		 uu.S_UCET, f.s_kod
   ]]></SQLQuery>
