<?xml version="1.0" encoding='windows-1250'?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">

<ViewObject
   Name="DatKurzlistExportView"
   OrderBy="DT_DATUM, TYP, BANKA, MENA"
   BindingStyle="Oracle"
   CustomQuery="true"
   ComponentClass="cz.jtbank.konsolidace.doklady.DatKurzlistExportViewImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
select l.DT_DATUM,
	   c.S_ZKRATKA banka,
	   l.ID_CISMENA mena,
	   l.ND_KURZ / l.NL_KVANTITA kurz,
	   0 typ
from DB_JT.DAT_KURZLIST l,
	 DB_JT.CIS_BANKA c
where l.DT_DATUM = last_day(l.DT_DATUM)
  and l.ID_CISBANKA = c.ID
union
select l.DT_DATUM,
	   c.S_ZKRATKA banka,
	   l.ID_CISMENA mena,
	   l.ND_KURZ / l.NL_KVANTITA kurz,
	   1 typ
from DB_JT.DAT_KURZLISTAVG l,
	 DB_JT.CIS_BANKA c
where l.DT_DATUM = last_day(l.DT_DATUM)
  and l.ID_CISBANKA = c.ID
   ]]></SQLQuery>
