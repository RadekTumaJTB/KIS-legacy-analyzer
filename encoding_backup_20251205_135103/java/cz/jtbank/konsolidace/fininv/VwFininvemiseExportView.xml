   Name="VwFininvemiseExportView"
   OrderBy="s_nazev"
   BindingStyle="Oracle"
   CustomQuery="true"
   ComponentClass="cz.jtbank.konsolidace.fininv.VwFininvemiseExportViewImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
select s.S_NAZEV,
	   s.S_ICO,
	   s.S_COUNTRY,
	   fi.S_MENA,
	   fi.S_ISIN,
	   emi.DT_PLATNOSTOD,
	   emi.DT_PLATNOSTDO,
	   emi.C_NENULOVY,
	   emi.NL_POCETKUSU,
	   emi.NL_NOMINAL,
	   emi.ND_ZAKLADNIJMENI,
	   emi.C_INVESTICE
from db_jt.kp_ktg_finInvesticeEmise emi,
	 db_jt.kp_ktg_financniInvestice fi,
	 db_jt.kp_ktg_spolecnost s
where emi.ID_KTGFINANCNIINVESTICE (+) = fi.ID
  and fi.ID_KTGSPOLECNOST = s.ID
  /*and sysdate between emi.DT_PLATNOSTOD and emi.DT_PLATNOSTDO*/
   ]]></SQLQuery>
