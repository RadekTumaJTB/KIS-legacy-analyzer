<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">

<ViewObject
   Name="VwDatBudgetpolozkadata1PARView"
   BindingStyle="JDBC"
   CustomQuery="true"
   ComponentClass="cz.jtbank.konsolidace.budget.VwDatBudgetpolozkadata1PARViewImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
select sum(
         case
           when (td.BUDGET_MENA = td.UCSPOL_MENA) then td.ND_CASTKALOCAL 
           else db_jt.kurzy.f_getMenaKurs ('ÈNB',td.BUDGET_MENA,td.UCSPOL_MENA,NULL) * td.ND_CASTKALOCAL
		 end
	   ) nd_castkalocal_zd,
       td.BUDGET_MENA s_mena_zd,
	   td.ID_BUDGET,
	   td.ID_CISTYPTRANSAKCE id_cistyptran
from DB_JT.VW_DAT_BUDGETTRANSAKCE td
where (td.ID_KTGPROJEKT=1000001 or td.ID_KTGPROJEKT is null)
  and td.SPOL_DOC = 0  
  and trunc(td.DATUM) between trunc(to_date(?),'yyyy') and to_date(?) 
and td.ID_BUDGET = ? and td.ID_CISTYPTRANSAKCE = ? 
group by td.BUDGET_MENA,
	   td.ID_BUDGET,
	   td.ID_CISTYPTRANSAKCE
   ]]></SQLQuery>
