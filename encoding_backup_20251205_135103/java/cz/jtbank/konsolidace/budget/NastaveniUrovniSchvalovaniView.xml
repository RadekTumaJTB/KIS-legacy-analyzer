   </ViewAttribute>
   <ViewAttribute
      Name="SNazev"
      IsPersistent="false"
      Precision="128"
      Type="java.lang.String"
      AliasName="S_NAZEV"
      ColumnType="VARCHAR2"
      Expression="S_NAZEV"
      SQLType="VARCHAR" >
      <DesignTime>
         <Attr Name="_DisplaySize" Value="128" />
      </DesignTime>
   </ViewAttribute>
   <ViewAttribute
      Name="NdBudgetnavyseniOo"
      IsPersistent="false"
