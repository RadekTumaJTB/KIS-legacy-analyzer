<ViewObject
   Name="KpDatZdaFlowZustatekLocalView"
   BindingStyle="Oracle"
   CustomQuery="true"
   ComponentClass="cz.jtbank.konsolidace.cartesis.KpDatZdaFlowZustatekLocalViewImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
select a.ID_DOKLAD,
	   f.ID id_cisFlow,
	   f.S_KOD,
	   uu.S_UCET,
	   sum(decode(uu.c_class,'A',1,'3',1,-1)*a.ND_CASTKALOCAL) ND_CASTKALOCAL
from db_jt.kp_dat_dokladZdrojDatAgregace a,
	 db_jt.kp_rel_ucetUnifFlow rel,
	 db_jt.kp_cis_flow f,
	 db_jt.kp_cis_ucetUnif uu
where rel.ID_CISUCETUNIF = a.ID_CISUCETUNIF
  and rel.ID_CISFLOW = f.ID
  and a.ID_CISUCETUNIF = uu.id
group by a.ID_DOKLAD,
	   f.ID,
	   f.S_KOD,
	   uu.S_UCET
   ]]></SQLQuery>
