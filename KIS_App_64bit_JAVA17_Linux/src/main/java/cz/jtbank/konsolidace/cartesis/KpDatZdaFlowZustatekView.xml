<ViewObject
   Name="KpDatZdaFlowZustatekView"
   OrderBy="s_ucet,s_mena"
   BindingStyle="Oracle"
   CustomQuery="true"
   ComponentClass="cz.jtbank.konsolidace.cartesis.KpDatZdaFlowZustatekViewImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
select a.ID_DOKLAD,
	   f.ID id_cisFlow,
	   f.S_KOD,
	   uu.S_UCET,
	   a.S_MENA,
	   sum(db_jt.f_getSignCartesis ( uu.c_class, dp.nl_sloupec )*a.ND_CASTKALOCAL) ND_CASTKALOCAL,
	   sum(db_jt.f_getSignCartesis ( uu.c_class, dp.nl_sloupec )*a.ND_CASTKAMENA) ND_CASTKAMENA
from db_jt.kp_dat_dokladZdrojDatAgregace a,
	 db_jt.kp_rel_ucetUnifFlow rel,
db_jt.kp_dat_dokladPolozka dp,
	 db_jt.kp_cis_flow f,
	 db_jt.kp_cis_ucetUnif uu, db_Jt.kp_dat_Doklad d
where rel.ID_CISUCETUNIF = a.ID_CISUCETUNIF
  and rel.ID_CISFLOW = f.ID
  and a.ID_CISUCETUNIF = uu.id
and dp.id = a.id_dokladpolozka
and d.id = a.id_doklad
--and db_jt.f_exportCartesis ( d.id, d.id_ktgUcetniSpolecnost, uu.id, a.id_ktgUcetniSpolecnost ) > 0
group by a.ID_DOKLAD,
	   f.ID,
	   f.S_KOD,
	   uu.S_UCET,
	   a.S_MENA, dp.nl_sloupec
   ]]></SQLQuery>
