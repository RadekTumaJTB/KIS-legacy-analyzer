<ViewObject
   Name="KpDatZdaFlowSegmentLocalView"
   BindingStyle="Oracle"
   CustomQuery="true"
   ComponentClass="cz.jtbank.konsolidace.cartesis.KpDatZdaFlowSegmentLocalViewImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
select /*+ordered*/
	   r.ID_DOKLAD,
	   f.ID id_cisFlow,
	   f.S_KOD,
	   uu.S_UCET,
	   sum(decode(uu.c_class,'A',1,'3',1,-1)*zd.ND_CASTKALOCAL) ND_CASTKALOCAL,
	   cis.ID_KTGSUBKONSOLIDACE
from db_jt.kp_rel_doklad_zdrojDat r,
	 db_jt.kp_dat_dokladZdrojDat zd,
	 db_jt.kp_rel_ucetUnifFlow rel,
	 db_jt.kp_cis_flow f,
	 db_jt.kp_cis_ucetUnif uu,
	 db_jt.kp_ktg_projekt p,
	 db_jt.kp_cis_mngSegment cis
where r.ID_DOKLADZDROJDAT = zd.ID
  and rel.ID_CISUCETUNIF = zd.ID_CISUCETUNIF
  and rel.ID_CISFLOW = f.ID
  and zd.ID_CISUCETUNIF = uu.id
  and uu.C_MULTISEGMENT = '1'
  and zd.ID_KTGPROJEKT = p.ID		 
  and p.ID_MNGSEGMENT = cis.ID	
group by r.ID_DOKLAD,
	   f.ID,
	   f.S_KOD,
	   uu.S_UCET,
	   cis.ID_KTGSUBKONSOLIDACE
   ]]></SQLQuery>
