<ViewObject
   Name="KpDatDokladzdrojdatagregaceAgr1View"
   OrderBy="s_ucet, s_mena"
   BindingStyle="Oracle"
   CustomQuery="true"
   ComponentClass="cz.jtbank.konsolidace.doklady.KpDatDokladzdrojdatagregaceAgr1ViewImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
select a.ID_DOKLAD,
	   a.S_UCET,
	   a.S_MENA,
	   sum(decode(c_list,'A',1,'3',1,-1)*a.ND_CASTKALOCAL) ND_CASTKALOCAL,
	   sum(decode(c_list,'A',1,'3',1,-1)*a.ND_CASTKAMENA) ND_CASTKAMENA,
      f.s_kod 
from 
	db_jt.kp_dat_dokladZdrojDatAgregace a, 
   db_jt.Kp_Cis_Ucetunif uf, 
	db_jt.kp_rel_ucetunifflow rf,
	db_jt.kp_cis_flow f
where 
	uf.s_ucet = a.s_ucetunif and
	rf.id_cisflow = f.id   and
	uf.id = rf.id_cisucetunif 
group by a.ID_DOKLAD,
	   	 a.S_UCET,
	   	 a.S_MENA,
          f.s_kod
   ]]></SQLQuery>
