<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">

<ViewObject
   Name="VwSubkonDokladpolozkaNewView"
   OrderBy="nl_poradilist, nl_radek, nl_sloupec"
   BindingStyle="JDBC"
   CustomQuery="true"
   ComponentClass="cz.jtbank.konsolidace.doklady.VwSubkonDokladpolozkaNewViewImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT   id_doklad,
              nd_castkalocal * 
decode(NL_PORADILIST,5,db_jt.kurzy.f_getMenaAvgKurs(?,?,s_mena,dt_datum),db_jt.kurzy.f_getMenaKurs(?,?,s_mena,dt_datum))
            * db_jt.f_getsignbilancelist (nl_poradilist) nd_castkalocal,
            nl_radek, nl_sloupec, nl_poradilist
       FROM (SELECT d.ID id_doklad, nd_castkalocal, nl_radek, nl_sloupec,
                    nl_poradilist, d.s_mena, d.dt_datum
               FROM db_jt.kp_dat_dokladpolozka dp, db_jt.kp_dat_doklad d
              WHERE d.ID = dp.id_doklad AND dp.s_mena = d.s_mena)
   ]]></SQLQuery>
