<?xml version="1.0" encoding='windows-1250'?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">

<ViewObject
   Name="WvDatBudgetpolozkadata2XPARView"
   BindingStyle="JDBC"
   CustomQuery="true"
   ComponentClass="cz.jtbank.konsolidace.budget.WvDatBudgetpolozkadata2XPARViewImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
select sum(
         case
           when (td.BUDGET_MENA = td.UCSPOL_MENA) then td.ND_CASTKALOCAL 
           else db_jt.kurzy.f_getMenaKurs ('ÈNB', td.BUDGET_MENA,td.UCSPOL_MENA,NULL) * td.ND_CASTKALOCAL
		 end
	   ) nd_castkalocal_sch,
       	td.BUDGET_MENA s_mena_sch,
	td.ID_BUDGET,
	   C_CAPEX_OPEX 
from DB_JT.VW_DAT_BUDGETTRANSAKCEDOC td
where (td.ID_KTGPROJEKT=1000001 or td.ID_KTGPROJEKT is null)
  and td.SPOL_DOC = 0  
  and trunc(td.DATUM) between trunc(to_date(?),'yyyy') and to_date(?)
and td.ID_BUDGET = ?
group by td.BUDGET_MENA,
	   td.ID_BUDGET,
 	   C_CAPEX_OPEX
   ]]></SQLQuery>
   <DesignTime>
      <Attr Name="_version" Value="9.0.4.13.65" />
      <Attr Name="_CodeGenFlagNew" Value="36" />
   </DesignTime>
   <ViewAttribute
      Name="NdCastkalokalSch"
      IsPersistent="false"
      Precision="255"
      Type="oracle.jbo.domain.Number"
      AliasName="ND_CASTKALOCAL_SCH"
      ColumnType="VARCHAR2"
      Expression="ND_CASTKALOCAL_SCH"
      SQLType="NUMERIC" >
   </ViewAttribute>
   <ViewAttribute
