<?xml version="1.0" encoding='windows-1250'?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">

<ViewObject
   Name="VwDatBudgetProjektpolozkadata2PARView"
   BindingStyle="JDBC"
   CustomQuery="true"
   ComponentClass="cz.jtbank.konsolidace.budget.VwDatBudgetProjektpolozkadata2PARViewImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
select sum(
         case
           when (td.BUDGET_MENA = td.UCSPOL_MENA) then td.ND_CASTKALOCAL 
           else db_jt.kurzy.f_getMenaKurs ('ÈNB',td.BUDGET_MENA,td.UCSPOL_MENA,NULL) * td.ND_CASTKALOCAL
		 end
	   ) nd_castkalocal_sch,
       td.BUDGET_MENA s_mena_sch,
	   td.ID_BUDGET,
	   td.ID_CISTYPTRANSAKCE id_cistyptran
from DB_JT.VW_DAT_BUDGETPROJTRANSAKCEDOC td
where trunc(td.DATUM) <= to_date(?)
and td.ID_BUDGET = ? and td.ID_CISTYPTRANSAKCE = ? 
and trunc(td.DATUM) >= (select dt_platnostOd from db_jt.kp_dat_budget where id = td.ID_BUDGET)
group by td.BUDGET_MENA,
	   td.ID_BUDGET,
	   td.ID_CISTYPTRANSAKCE
   ]]></SQLQuery>
   <DesignTime>
