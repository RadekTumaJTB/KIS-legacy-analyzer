<ViewObject
   Name="VwRelUcetniSpolecnostSkupinaView"
   OrderBy="s_nazev"
   BindingStyle="Oracle"
   CustomQuery="true"
   ComponentClass="cz.jtbank.konsolidace.subkons.VwRelUcetniSpolecnostSkupinaViewImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT distinct us.s_nazev, us.id, us.s_ico, c.id_ktgucetniskupina
     FROM db_jt.kp_rel_subkonsolidaceclen c,
          db_jt.kp_ktg_subkonsolidace s,
          db_jt.kp_ktg_ucetnispolecnost us
    WHERE us.ID = c.id_ktgucetnispolecnost
      AND s.id_ktgucetnispolecnost = c.id_ktgsubkonsolidace
AND c.c_clen = '1' and ( (sysdate between c.dt_clenstviOd and c.dt_clenstviDo) or ( sysdate - c.dt_clenstviDo < 365 ) )
   ]]></SQLQuery>
