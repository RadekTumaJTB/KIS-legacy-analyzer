<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">

<ViewObject
   Name="VwInvesticeZakladnijmeni"
   OrderBy="ID"
   BindingStyle="JDBC"
   CustomQuery="true"
   ComponentClass="cz.jtbank.konsolidace.majetek.VwInvesticeZakladnijmeniImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
select fi.ID, sum(emi.ND_ZAKLADNIJMENI) zj
from db_jt.kp_ktg_financniinvestice fi,
	 db_jt.kp_ktg_fininvesticeemise emi
where fi.ID = emi.ID_KTGFINANCNIINVESTICE
  and ? between emi.DT_PLATNOSTOD and emi.DT_PLATNOSTDO	
group by fi.ID
   ]]></SQLQuery>
